{
    "name": "mhrebinnyk/ip-clock",
    "version": "0.1.1",
    "description": "A PSR-20 Clock implementation that returns time based on server external IP address using external API.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hrebinnyk Maksym",
            "email": "grebennik.maxim@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "psr/clock": "^1.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Mhrebinnyk\\IpClock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mhrebinnyk\\IpClock\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable"
}
